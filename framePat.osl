shader framePat(
	float depth=0,
	float width=0,
	float fuzz=0.02,
	float where=0.5,
	float repeatCount=8,
	output float resultF=0
						)
{
	float dist;
    float inDisk;

	
	
	dist=abs(v-where);
    inDisk =smoothstep(width-fuzz,width+fuzz,dist);
	resultF=inDisk*depth;
    resultF*=(smoothstep(0.5-fuzz,0.5+fuzz,v));
}